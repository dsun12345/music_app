<%= form_for @song do |f| %>
 
    <%= render partial: "/partials/errors", locals: {obj: @song} %>

    <%= f.label :title %>
    <%= f.text_field :title %> 

    <%= f.label :artist %>
    <%= f.text_field :artist %> 

    <%= f.collection_select(:playlist_id, Playlist.all, :id, :title) %>

    <%= f.submit "Create Song!!" %> 

<% end %> 